class Solution(object):
    def largestGoodInteger(self, num):
        """
        :type num: str
        :rtype: str
        """
        maxi = -1
        for i in range(1, len(num) - 1):
            if num[i] == num[i-1] and num[i] == num[i+1]:
                maxi = max(maxi, int(num[i]))

        if maxi == -1:
            return ""
        
        return str(maxi)+str(maxi)+str(maxi)
